.content .form .input label, .content .form .input .memberNo, .content .form .input view input, .content .form .input view text, .content .form .sex-box radio-group, .content .form .sex-box radio-group .radio, .content .form .sex-box .sexLabel, .content .form .car-box .car-title, .content .form .car-box .card-box .add-btn, .content .form .car-box .cardNo-box .cardNo, .content .form .btn-box button {
  height: 80rpx;
  line-height: 80rpx;
}
.content .form .input .memberNo, .content .form .input view, .content .form .car-box .card-box, .content .form .btn-box button {
  border-radius: 12rpx;
}
.content {
  padding-top: 40rpx;
}
.content .form {
    width: 80%;
    margin: 0 auto;
    font-size: 32rpx;
}
.content .form .input {
      display: block;
      margin-top: 40rpx;
      height: 80rpx;
}
.content .form .input label {
        float: left;
        width: 140rpx;
        text-align: right;
        margin-right: 20rpx;
}
.content .form .input .memberNo {
        float: left;
        width: calc(100% - 180rpx);
        border: 1px solid #333;
        padding-left: 10rpx;
}
.content .form .input view {
        float: left;
        height: 80rpx;
        width: calc(100% - 180rpx);
        border: 1px solid #333;
}
.content .form .input view input {
          float: left;
          padding-left: 10rpx;
          width: calc(100% - 70rpx);
}
.content .form .input view text {
          float: right;
          font-size: 24px;
          margin-right: 10rpx;
          color: #666;
}
.content .form .sex-box {
      width: 100%;
      height: 80rpx;
      margin-top: 40rpx;
}
.content .form .sex-box radio-group {
        display: inline-block;
}
.content .form .sex-box .sexLabel {
        display: inline-block;
        width: 140rpx;
        text-align: right;
}
.content .form .memberLevel {
      display: block;
      margin-top: 40rpx;
}
.content .form .memberLevel text {
        display: inline-block;
        width: 160rpx;
        vertical-align: top;
}
.content .form .memberLevel .member-info {
        display: inline-block;
        width: calc(100% - 180rpx);
}
.content .form .memberLevel .member-info .checkbox-item {
          display: block;
          margin-bottom: 20rpx;
}
.content .form .car-box {
      margin-top: 40rpx;
}
.content .form .car-box .car-title image {
        width: 40rpx;
        height: 40rpx;
        float: right;
        margin-top: 20rpx;
}
.content .form .car-box .card-box {
        border: 1px solid #333;
}
.content .form .car-box .card-box .add-btn {
          text-align: center;
          color: #999;
}
.content .form .car-box .cardNo-box .cardNo {
        margin: 0 10rpx;
        padding: 0 10rpx;
        border-bottom: 1px solid #666;
}
.content .form .car-box .cardNo-box .cardNo image {
          width: 40rpx;
          height: 40rpx;
          float: right;
          margin-top: 20rpx;
}
.content .form .btn-box {
      height: 80rpx;
      margin: 40rpx 0;
}
.content .form .btn-box button {
        border: 1px solid #333;
        width: 240rpx;
}
.content .form .btn-box button.left {
        float: left;
}
.content .form .btn-box button.right {
        float: right;
}

