<view class="content" bind:__l="__l"><view class="service-detail"><view class="_p">服务详情</view><view class="input-row"><label class="cellname">服务项目</label><input type="text" disabled placeholder="请选择" data-event-opts="{{[['tap',[['showBoxHandler',[1]]]]]}}" bindtap="__e"/></view><view class="input-row"><label class="cellname">是否加急</label><radio-group><label class="radio"><radio value="r1" checked="true"></radio>否</label><label class="radio"><radio value="r2"></radio>是</label></radio-group></view><view class="input-row"><label class="cellname">预计取件日期</label><input type="text" disabled placeholder="请选择" data-event-opts="{{[['tap',[['selectDate',['$event']]]]]}}" bindtap="__e"/></view><view class="input-row"><label class="cellname">备注</label><input type="text" placeholder="请输入" disabled="{{type==3}}"/></view></view><view class="article-detail"><view class="_p">物件详情</view><view class="input-row"><label class="cellname">品类</label><input type="text" disabled placeholder="请选择" data-event-opts="{{[['tap',[['showBoxHandler',[2]]]]]}}" bindtap="__e"/></view><view class="input-row"><label class="cellname">品牌</label><input type="text" placeholder="请输入" disabled="{{type==3}}"/></view><view class="input-row"><label class="cellname">颜色</label><input type="text" placeholder="请输入" disabled="{{type==3}}"/></view><view class="input-row"><label class="cellname">附带物件</label><input type="text" disabled placeholder="请选择" data-event-opts="{{[['tap',[['showBoxHandler',[3]]]]]}}" bindtap="__e"/></view><view class="input-row"><label class="cellname">不良效果</label><input type="text" disabled placeholder="请选择" data-event-opts="{{[['tap',[['showBoxHandler',[4]]]]]}}" bindtap="__e"/></view><view class="input-row"><label class="cellname">物件瑕疵</label><input type="text" disabled placeholder="请选择" data-event-opts="{{[['tap',[['showBoxHandler',[5]]]]]}}" bindtap="__e"/></view><block wx:if="{{type!=3}}"><view class="upload-row"><label>物件照片</label><view class="img-box"><block wx:for="{{imgs}}" wx:for-item="img" wx:for-index="index" wx:key="index"><image src="{{img.path}}"></image></block><block wx:if="{{imgs.length<6}}"><text data-event-opts="{{[['tap',[['clickUpload',['$event']]]]]}}" class="uni-icon uni-icon-plusempty" bindtap="__e"></text></block></view></view></block></view><view class="bottom"><button data-event-opts="{{[['tap',[['backPage',['$event']]]]]}}" bindtap="__e">{{type!=3?'取消':'返回'}}</button><block wx:if="{{type!=3}}"><button data-event-opts="{{[['tap',[['clickConfirm',['$event']]]]]}}" bindtap="__e">确定</button></block><block wx:else><button data-event-opts="{{[['tap',[['toArticleStatus',['$event']]]]]}}" bindtap="__e">物件状态</button></block></view><block wx:if="{{showBox}}"><view data-event-opts="{{[['touchmove',[['',['$event']]]],['tap',[['hideMask',['$event']]]]]}}" class="mask" catchtouchmove="__e" bindtap="__e"></view></block><block wx:if="{{showBox}}"><view class="select-box"><block wx:if="{{boxType==1}}"><block><checkbox-group class="single-box"><block wx:for="{{itemList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><label class="checkbox-item"><checkbox value="{{index+''}}" name="item"></checkbox>{{item.name+''}}</label></block></checkbox-group></block></block><block wx:else><block wx:if="{{boxType==2}}"><block><radio-group class="double-box"><block wx:for="{{categories}}" wx:for-item="item" wx:for-index="categoryIndex" wx:key="categoryIndex"><label class="radio-item"><radio value="{{categoryIndex+''}}" name="category"></radio>{{item.name+''}}</label></block></radio-group></block></block><block wx:else><block wx:if="{{boxType==3}}"><block><radio-group class="single-box"><block wx:for="{{attachments}}" wx:for-item="item" wx:for-index="attachmentIndex" wx:key="attachmentIndex"><label class="radio-item"><radio value="{{attachmentIndex+''}}" name="attachment"></radio>{{item.name+''}}<block wx:if="{{attachmentIndex==attachments.length-2}}"><input type="text" value="" placeholder="请输入"/></block></label></block></radio-group></block></block><block wx:else><block wx:if="{{boxType==4}}"><block><checkbox-group class="single-box"><block wx:for="{{effects}}" wx:for-item="item" wx:for-index="effectIndex" wx:key="effectIndex"><label class="radio-item"><checkbox value="{{effectIndex+''}}" name="attachment"></checkbox>{{item.name+''}}<block wx:if="{{effectIndex==effects.length-1}}"><input type="text" value="" placeholder="请输入"/></block></label></block></checkbox-group></block></block><block wx:else><block wx:if="{{boxType==5}}"><block><checkbox-group class="double-box"><block wx:for="{{flaws}}" wx:for-item="item" wx:for-index="flawIndex" wx:key="flawIndex"><label class="checkbox-item"><checkbox value="{{flawIndex+''}}" name="attachment"></checkbox>{{item.name+''}}<block wx:if="{{flawIndex==flaws.length-1}}"><input type="text" value="" placeholder="请输入"/></block></label></block></checkbox-group></block></block></block></block></block></block><view class="btn-box"><button data-event-opts="{{[['tap',[['hideMask',['$event']]]]]}}" bindtap="__e">取消</button><button data-event-opts="{{[['tap',[['confirmSelect',['$event']]]]]}}" bindtap="__e">确定</button></view></view></block></view>